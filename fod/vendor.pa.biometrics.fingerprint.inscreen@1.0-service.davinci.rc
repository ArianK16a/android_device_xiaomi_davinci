on boot
    chown system system /sys/devices/platform/soc/soc:qcom,dsi-display/fod_ui
    chmod 0444 /sys/devices/platform/soc/soc:qcom,dsi-display/fod_ui

service vendor.fingerprint-inscreen-1-0 /vendor/bin/hw/vendor.pa.biometrics.fingerprint.inscreen@1.0-service.davinci
    interface vendor.pa.biometrics.fingerprint.inscreen@1.0::IFingerprintInscreen default
    class hal
    user system
    group system
    shutdown critical
